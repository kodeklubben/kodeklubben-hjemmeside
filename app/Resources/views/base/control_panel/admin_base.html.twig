<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{% block title %}Kodeklubben - Kontrollpanel{% endblock %}</title>

    {% block stylesheets %}{% endblock %}
    <!-- Custom style -->
    <link rel="stylesheet" href="{{ asset('css/app.css') }}">
    <link rel="stylesheet" href="{{ asset('css/admin_app.css') }}">
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />

</head>
<body class="hold-transition skin-blue">

{% include 'base/control_panel/admin_navigation.html.twig' %}

<div class="wrapper">
    <!-- Main header. contains mobile navigation button -->
    {{ include('base/control_panel/main_header.html.twig') }}
    <!-- Left side column. contains sidebar -->
    {{ include('base/control_panel/main_sidebar.html.twig') }}
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        {% block body %}{% endblock %}
    </div>
</div>
<script src="{{ asset('js/jquery.min.js') }}"></script>
<script src="{{ asset('js/bootstrap.min.js') }}"></script>
<script src="{{ asset('js/app.js') }}"></script>
<script src="{{ asset('js/admin_app.js') }}"></script>
<script src="{{ asset('js/sorttable.js') }}"></script>
{% block javascripts %}{% endblock %}
<script>
    {% if google_analytics_id is defined and google_analytics_id | length > 0 %}
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', '{{ google_analytics_id }}', 'auto');
        ga('send', 'pageview');
    {% endif %}
</script>
</body>
</html>
